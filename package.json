{
  "name": "piratesofthescandinavian",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Playable Act 2 runtime for Pirates of the Scandinavian",
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit -p tsconfig.app.json && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "export:csv:act2": "node scripts/export-pack-csv.mjs --pack story/data/act2-quest-pack.json --out story/csv/act2",
    "export:csv:act3": "node scripts/export-pack-csv.mjs --pack story/data/act3-quest-pack.json --out story/csv/act3",
    "export:csv:all": "npm run export:csv:act2 && npm run export:csv:act3"
  },
  "dependencies": {
    "ajv": "^8.17.1",
    "framer-motion": "^12.34.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.4",
    "autoprefixer": "^10.4.24",
    "jsdom": "^28.0.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  }
}
